/*动画部分*/
.coin-ani{
 animation: tiao 1.5s linear infinite;
}
@keyframes tiao{
  0%{
    transform: translateY(0)
  }
  50%{
    transform: translateY(-15rpx)
  }
  100%{
    transform: translateY(0)
  }
}
.coin-fly1{
   animation: move1 1.5s ease-in-out ;
}
.coin-fly2{
   animation: move2 1.5s ease-in-out ;
}
.coin-fly3{
   animation: move3 1.5s ease-in-out ;
}
.coin-fly4{
   animation: move4 1.5s ease-in-out ;
}
@keyframes tiao{
  0%{
    transform: translateY(0)
  }
  50%{
    transform: translateY(-15rpx)
  }
  100%{
    transform: translateY(0)
  }
}
@keyframes move1{
  from{
    transform: translate3d(0,0,0) scale(1);
    opacity:1;
  }
  to{
    transform: translate3d(250rpx,100rpx,-50rpx) scale(0.3);
    opacity: 0;
  }
}
@keyframes move2{
  from{
    transform: translate3d(0,0,0) scale(1);
    opacity:1;
  }
  to{
    transform: translate3d(-250rpx,100rpx,-50rpx) scale(0.3);
    opacity: 0;
  }
}
@keyframes move3{
  from{
    transform: translate3d(0,0,0) scale(1);
    opacity:1;
  }
  to{
    transform: translate3d(250rpx,-100rpx,-50rpx) scale(0.3);
    opacity: 0;
  }
}
@keyframes move4{
  from{
    transform: translate3d(0,0,0) scale(1);
    opacity:1;
  }
  to{
    transform: translate3d(-250rpx,-100rpx,-50rpx) scale(0.3);
    opacity: 0;
  }
}
